{
  "name": "@commons/types",
  "version": "1.0.0",
  "description": "Shared branded types for type safety across commons.systems",
  "type": "module",
  "main": "dist/branded.js",
  "types": "dist/branded.d.ts",
  "exports": {
    "./branded": {
      "types": "./dist/branded.d.ts",
      "default": "./dist/branded.js"
    }
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc && node -e \"['dist/branded.js','dist/branded.d.ts','dist/branded.js.map'].forEach(f=>{if(!require('fs').existsSync(f))throw new Error('Missing: '+f)})\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "types",
    "branded-types",
    "type-safety"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
